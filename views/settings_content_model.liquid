import QtQuick 2.0

ListModel{
{%- for item in content_items %}
  ListElement{
    label: "{{item.content_label}}"
    parentLabel: "{{item.parent_name}}"
    parentLevel: {{item.parent_level}}
    interactionComponent: "{{item.control_type}}"
    values: [
    {%- for value in item.values %}
      ListElement{value: "{{value}}"}
      {%- if value != item.values.last -%},{%- endif -%} 
    {% endfor %}
    ]
    state: 0
  }
{%- endfor %}
}
