import QtQuick 2.0

ListModel{
  property int selectedIndex: -1
{%- for category in hierarchy_levels %}
  ListElement{
    label: "{{category.name}}"
    parentLabel: "{{category.parent}}"
    childLabels: [
    {%- for child in category.children %}
      ListElement{childLabel: "{{child}}"}
      {%- if child != category.children.last -%},{%- endif -%}
    {% endfor %}
    ]
  }
{%- endfor %}
}
